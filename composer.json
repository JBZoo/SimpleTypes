{
    "name"             : "smetdenis/simpletypes",
    "type"             : "library",
    "description"      : "The universal PHP library to convert any values and measures",
    "license"          : "MIT",
    "minimum-stability": "dev",
    "keywords"         : [
        "converter",
        "acceleration",
        "area",
        "degree",
        "info",
        "length",
        "money",
        "number",
        "pressure",
        "speed",
        "temperature",
        "time",
        "volume",
        "weight"
    ],
    "authors"          : [
        {
            "name" : "SmetDenis",
            "email": "denis@jbzoo.com"
        }
    ],
    "require"          : {
        "php": ">=5.3.1"
    },
    "require-dev"      : {
        "phpunit/phpunit"          : "4.7.*",
        "phpunit/php-code-coverage": "2.1.*",
        "satooshi/php-coveralls"   : "0.6.*",
        "squizlabs/php_codesniffer": "2.3.*",
        "sebastian/phpcpd"         : "*"
    },
    "autoload"         : {
        "classmap": [
            "src"
        ]
    },
    "support"          : {
        "issues": "https://github.com/smetdenis/SimpleTypes/issues"
    }
}
